{
    "compilerOptions": {
        "outDir": "./dist",
        "noImplicitAny": false,
        "jsx": "react",
        "baseUrl": ".",
        "rootDir": ".",
        "sourceMap": true,
        "target": "es5",
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "newLine": "lf",
        "noFallthroughCasesInSwitch": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "removeComments": true,
        "strictNullChecks": true
    }
}
