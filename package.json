{
  "name": "brackets-terminal-x",
  "title": "Brackets Terminal X",
  "version": "0.6.0",
  "description": "Integrate the terminal of your choice.",
  "main": "main.js",
  "homepage": "https://github.com/ficristo/brackets-terminal-x",
  "repository": "https://github.com/ficristo/brackets-terminal-x",
  "keywords": [
    "brackets",
    "brackets-extension",
    "terminal",
    "xterm",
    "node-pty",
    "node-pty-prebuilt"
  ],
  "author": "ficristo",
  "license": "MIT",
  "engines": {
    "brackets": "^1.11.0"
  },
  "scripts": {
    "build": "rollup -c rollup.config.ts",
    "dev": "tsc --project tsconfig.json && tsc --project src/node/tsconfig.json",
    "lint:js": "eslint . --ignore-path .gitignore --ext=js,ts",
    "lint": "npm run lint:js",
    "prepush": "npm run test",
    "test": "npm run lint",
    "dist": "npm run dev && npm run test",
    "preversion": "npm run dist",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "node-pty-prebuilt": "^0.7.6",
    "xterm": "^3.13.0"
  },
  "devDependencies": {
    "@types/jquery": "^2.0.53",
    "@types/mustache": "^0.8.32",
    "@types/node": "^6.14.6",
    "@types/requirejs": "^2.1.31",
    "eslint": "^5.16.0",
    "eslint-config-moody-tsx": "^0.6.0",
    "husky": "0.14.3",
    "rollup": "^1.11.3",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-node-resolve": "^4.2.4",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.21.0",
    "typescript": "~3.5.2"
  }
}
